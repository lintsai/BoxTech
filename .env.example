# Environment Variables for BoxTech

# Application
APP_NAME=BoxTech
APP_ENV=development
DEBUG=True
SECRET_KEY=your-secret-key-here-change-in-production

# Database
DATABASE_URL=postgresql://boxtech:boxtech_password@localhost:5432/boxtech
DB_ECHO=False

# Redis
REDIS_URL=redis://localhost:6379/0

# Celery
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# OpenAI
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview

# AWS S3 (optional, for cloud storage)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=ap-northeast-1
S3_BUCKET_NAME=boxtech-videos

# Pinecone (optional, for vector database)
PINECONE_API_KEY=your-pinecone-api-key
PINECONE_ENVIRONMENT=us-west1-gcp
PINECONE_INDEX_NAME=boxing-actions

# Video Processing
VIDEO_UPLOAD_DIR=./Midea
VIDEO_TEMP_DIR=./temp
MAX_VIDEO_SIZE_MB=500

# MediaPipe
MEDIAPIPE_MODEL_COMPLEXITY=2
MEDIAPIPE_MIN_DETECTION_CONFIDENCE=0.5
MEDIAPIPE_MIN_TRACKING_CONFIDENCE=0.5

# API Settings
API_PREFIX=/api/v1
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Monitoring
SENTRY_DSN=your-sentry-dsn-here

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# JWT
JWT_SECRET_KEY=your-jwt-secret-key-here
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=60

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000
